#include<BoeBot.h>
#include<uart.h>
void running(counter,delay1,delay2)
{
int i;
for(i=0;i<counter;i++)
{
P1_1=1;
delay_nus(delay1);
P1_1=0;
P1_0=1;
delay_nus(delay2);
P1_0=0;
delay_nms(20);
}
int main()
{
uart_Init();
running(65,1700,1300);
running(19,1700,1700);
running(65,1300,1700);
running(19,1300,1300);
while(1);
return(0);
}
